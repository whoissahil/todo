  "bugs": {
    "url": "https://github.com/whoissahil/todo/issues"
  },
  "homepage": "https://github.com/whoissahil/todo#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.10"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
