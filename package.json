{
  "name": "todo",
  "version": "1.0.0",
  "description": "A basic todo app using MERN stack deployed on Azure",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoissahil/todo.git"
  },
